{
    "name": "eosis-hack",
    "version": "1.0.0",
    "description": "Project Made for EOSIS Hackathon",
    "main": "index.js",
    "scripts": {
        "dev": "npm run webpack-dev-server -- --host 0.0.0.0 --hot --env.mode development",
        "prod": "npm run webpack -- --env.mode production",
        "server": "npm run webpack -- --env.mode server",
        "webpack": "webpack",
        "webpack-dev-server": "webpack-dev-server",
        "prod:typescript": "npm run prod -- --env.presets typescript",
        "prod:analyze": "npm run prod -- --env.presets bundleanalyze",
        "prod:visualize": "npm run prod -- --env.presets bundlevisualizer",
        "prod:compress": "npm run prod -- --env.presets compress",
        "prod:debug": "npm run debug -- --env.mode production",
        "dev:debug": "npm run debug -- --env.mode development",
        "test": "jest",
        "docker:singleDev": "./rundockerdev.sh",
        "docker:singleProd": "./rundocker.sh",
        "docker:multiDevUsingMongo": "docker-compose -f dev.dockerfile",
        "docker:multiProdUsingMongo": "docker-compose",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.{js,jsx}\"",
        "build:backend": "tsc",
        "start": "node ./build/server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rachit2501/EOSIS-Hack.git"
    },
    "author": "Rachit Anand",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/rachit2501/EOSIS-Hack/issues"
    },
    "homepage": "https://github.com/rachit2501/EOSIS-Hack#readme",
    "dependencies": {
        "@overnightjs/core": "^1.6.15",
        "@overnightjs/logger": "^1.1.10",
        "@reach/router": "^1.3.3",
        "babel-plugin-transform-html-import-require-to-string": "0.0.3",
        "http-status-codes": "^1.4.0",
        "lint-staged": "^10.2.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "ts-jest": "^25.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-export-default-from": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "@babel/runtime": "^7.9.6",
        "@types/express": "^4.17.6",
        "@types/reach__router": "^1.3.5",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-import": "^1.13.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^3.1.0",
        "css-loader": "^3.5.3",
        "cssnano": "^4.1.10",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-html": "^6.0.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^4.0.0",
        "file-loader": "^6.0.0",
        "fork-ts-checker-webpack-plugin": "^4.1.3",
        "html-loader": "^1.1.0",
